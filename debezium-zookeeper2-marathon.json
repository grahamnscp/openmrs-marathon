{
  "id": "/dev/dataservices/zookeeper",
  "cpus": 1,
  "disk": 0,
  "mem": 512,
  "instances": 1,
  "maxLaunchDelaySeconds": 300,
  "gpus": 0,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 2181,
        "servicePort": 10102
      },
      {
        "containerPort": 2888,
        "servicePort": 10103
      },
      {
        "containerPort": 3888,
        "servicePort": 10104
      }
    ],
    "type": "MESOS",
    "volumes": [],
    "docker": {
      "image": "debezium/zookeeper:0.9.5.Final",
      "forcePullImage": false,
      "parameters": []
    }
  },
  "networks": [
    {
      "name": "dcos",
      "mode": "container"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "healthChecks": [],
  "fetch": [],
  "constraints": []
}
