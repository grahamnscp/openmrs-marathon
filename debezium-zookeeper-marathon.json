{
        "id": "/dev/dataservices/zookeeper",
        "cpus": 1.0,
        "mem": 512,
        "instances": 1,
        "acceptedResourceRoles": [
          "*"
        ],
        "container": {
            "type": "DOCKER",
            "docker": {
                "image": "debezium/zookeeper:0.9.5.Final",
                "network": "BRIDGE",
                "portMappings": [
                    {
                        "containerPort": 2181,
                        "protocol": "tcp"
                    },
                    {
                        "containerPort": 2888,
                        "protocol": "tcp"
                    },
                    {
                        "containerPort": 3888,
                        "protocol": "tcp"
                    }
                ]
            }
        },
        "fetch": [],
        "healthChecks": [
            {
                "gracePeriodSeconds": 300,
                "intervalSeconds": 60,
                "maxConsecutiveFailures": 3,
                "path": "/",
                "portIndex": 0,
                "protocol": "HTTP",
                "timeoutSeconds": 20
            }
        ]
}
