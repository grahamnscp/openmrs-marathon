{
        "id": "/dev/dataservices/kafka-topics-ui",
        "cpus": 1.0,
        "mem": 512,
        "instances": 1,
        "acceptedResourceRoles": [
          "*"
        ],
        "container": {
            "type": "DOCKER",
            "docker": {
                "forcePullImage": true,
                "image": "landoop/kafka-topics-ui:0.9.4",
                "network": "BRIDGE",
                "portMappings": [
                    {
                        "containerPort": 8000,
                        "protocol": "tcp"
                    }
                ]
            }
        },
        "env": {
            "KAFKA_REST_PROXY_URL": "http://kafka-rest-proxy:8082/",
            "PROXY": "true"
        },
        "fetch": [],
        "healthChecks": [
            {
                "gracePeriodSeconds": 300,
                "intervalSeconds": 60,
                "maxConsecutiveFailures": 3,
                "path": "/",
                "portIndex": 0,
                "protocol": "HTTP",
                "timeoutSeconds": 20
            }
        ]
}
