{
  "id": "/dev/dataservices/kafka-topics-ui",
  "cpus": 1,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "fetch": [],
  "constraints": [],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "landoop/kafka-topics-ui:0.9.4",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    },
    "portMappings": [
      {
        "containerPort": 8000,
        "hostPort": 8000,
        "protocol": "tcp",
        "name": "ktopicuiweb",
        "labels": {
          "VIP_0": "/dev/dataservices/kafka-topics-ui:8000"
        }
      }
    ],
    "volumes": []
  },
  "env": {
    "KAFKA_REST_PROXY_URL": "http://devdataservicesrest-proxy.marathon.l4lb.thisdcos.directory:8082",
    "PROXY": "true",
    "PROXY_SKIP_VERIFY": "true"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 10,
      "ignoreHttp1xx": false,
      "intervalSeconds": 5,
      "maxConsecutiveFailures": 10,
      "portIndex": 0,
      "timeoutSeconds": 10,
      "delaySeconds": 15,
      "protocol": "HTTP",
      "path": "/",
      "ipProtocol": "IPv4"
    }
  ],
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "labels": {
    "HAPROXY_0_VHOST": "kafkatopicsui.example.com",
    "HAPROXY_GROUP": "external"
  }
}
